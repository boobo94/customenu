{
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "no-await-in-loop": "off",
    "import/named": "off"
  }
}
